<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:width="300dp"
        android:height="300dp">
        <shape android:shape="rectangle" />
    </item>

    <item
        android:width="3dp"
        android:height="18dp"
        android:gravity="top|left">
        <shape android:shape="rectangle">
            <solid
                android:width="1dp"
                android:color="#9900E5FF" />
            <corners android:topLeftRadius="50dp" android:topRightRadius="50dp"/>
        </shape>
    </item>

    <item
        android:width="18dp"
        android:height="3dp"
        android:gravity="top|left">
        <shape android:shape="rectangle">
            <solid
                android:width="1dp"
                android:color="#9900E5FF" />
            <corners android:topLeftRadius="50dp" android:bottomLeftRadius="50dp"/>
        </shape>
    </item>

    <item
        android:width="3dp"
        android:height="18dp"
        android:gravity="top|right">
        <shape android:shape="rectangle">
            <solid
                android:width="1dp"
                android:color="#9900E5FF" />
            <corners android:radius="20dp"/>
        </shape>
    </item>

    <item
        android:width="18dp"
        android:height="3dp"
        android:gravity="top|right">
        <shape android:shape="rectangle">
            <solid
                android:width="1dp"
                android:color="#9900E5FF" />
            <corners android:radius="20dp"/>
        </shape>
    </item>


    <item
        android:width="3dp"
        android:height="18dp"
        android:gravity="bottom|left">
        <shape android:shape="rectangle">
            <solid
                android:width="1dp"
                android:color="#9900E5FF" />
            <corners android:radius="20dp"/>
        </shape>
    </item>

    <item
        android:width="18dp"
        android:height="3dp"
        android:gravity="bottom|left">
        <shape android:shape="rectangle">
            <solid
                android:width="1dp"
                android:color="#9900E5FF" />
            <corners android:radius="20dp"/>
        </shape>
    </item>


    <item
        android:width="3dp"
        android:height="18dp"
        android:gravity="bottom|right">
        <shape android:shape="rectangle">
            <solid
                android:width="1dp"
                android:color="#9900E5FF" />
            <corners android:radius="20dp"/>
        </shape>
    </item>

    <item
        android:width="18dp"
        android:height="3dp"
        android:gravity="bottom|right">
        <shape android:shape="rectangle">
            <solid
                android:width="1dp"
                android:color="#9900E5FF" />
            <corners android:radius="20dp"/>
        </shape>
    </item>


</layer-list>